{
  "emulators": {
    "functions": {
      "port": "5003"
    },
    "hosting": {
      "public": "public"
    }
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/tracker",
        "function": "tracker"
      },
      {
        "source": "/query",
        "function": "query"
      },
      {
        "source": "/collect",
        "function": "collect"
      },
      {
        "source": "/purgeAll",
        "function": "purgeAll"
      },
      {
        "source": "/purge",
        "function": "purge"
      },
      {
        "source": "/login",
        "function": "login"
      },
      {
        "source": "/logout",
        "function": "logout"
      },
      {
        "source": "/dashboard",
        "function": "dashboard"
      },
      {
        "source": "/reports/speed",
        "function": "speed"
      },
      {
        "source": "/reports/browsers",
        "function": "browsers"
      },
      {
        "source": "/reports/events",
        "function": "events"
      },
      {
        "source": "/register",
        "function": "register"
      },
      {
        "source": "/getUsers",
        "function": "getUsers"
      },
      {
        "source": "/checkUserType",
        "function": "checkUserType"
      },
      {
        "source": "/registerAdminUser",
        "function": "registerAdminUser"
      },
      {
        "source": "/deleteUser",
        "function": "deleteUser"
      },
      {
        "source": "/updateUser",
        "function": "updateUser"
      },
      {
        "source": "/createUser",
        "function": "createUser"
      },
      {
        "source": "/app",
        "function": "app"
      },
      {
        "source": "/sessionLogin",
        "function": "sessionLogin"
      }
    ]
  }
}
